{
  "extends": "@jessety",
  "rules": {
    "linebreak-style": ["warn", "unix"]
  },
  "overrides": [
    {
      "files": ["**/*.{js,cjs,mjs}"],
      "extends": "@jessety/eslint-config/javascript"
    },
    {
      "files": ["**/*.test.{ts,js,cjs,mjs}"],
      "env": { "jest": true },
      "rules": { "no-console": "warn" }
    }
  ]
}
